        <name>UserDetailUtil</name>
        <script><![CDATA[var UserDetailUtil = Class.create();
UserDetailUtil.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
    getUserDetail: function() {
        var userObj = {};
        var gr = GlideRecord('sys_user');
        if (gr.get(this.getParameter('sysparm_userSelect'))) {
            userObj.mail = gr.getValue('email');
        }
        return JSON.stringify(userObj);
    },
    type: 'UserDetailUtil'
});]]></script>
