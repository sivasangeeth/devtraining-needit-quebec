        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }
    var user = g_form.getValue('u_requested_for');
    var ga = new GlideAjax('EmailAutoPopulate');
    ga.addParam('sysparm_name', 'getEmailAutopopulate');
    ga.addParam('sysparm_userSelect',user);
    ga.getXMLAnswer(function(response) {
        var userObj = JSON.parse(response);
        g_form.setValue('u_requested_for_email', userObj.loccity);
    });
    //Type appropriate comment here, and begin script below

}]]></script>
